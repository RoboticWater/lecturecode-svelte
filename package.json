{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "cross-env": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "codemirror": "^5.48.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "gridfs-stream": "^1.1.1",
    "md5": "^2.2.1",
    "mongoose": "^5.8.5",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.1",
    "nprogress": "^0.2.0",
    "sirv-cli": "^0.4.4",
    "socket.io": "^2.3.0",
    "mongodb": "^3.1.1",
    "mongoose-gridfs": "^0.4.1",
    "method-override": "^3.0.0",
    "mongoose-id-validator": "^0.4.3"
  },
  "scripts": {
    "build": "babel server -d dist --ignore server/tests --copy-files && rollup -c",
    "dev": "run-p start:dev autobuild",
    "start:dev": "sirv public --single --dev",
    "server": "babel-node ./server/server.js",
    "start": "npm run build & npm run server",
    "watch": "babel -w server -d dist --ignore server/tests --copy-files",
    "serve": "node ./dist/server.js"
  }
